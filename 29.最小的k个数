最小的k个数
输入n个整数，找出其中最小的K个数。例如输入4,5,1,6,2,7,3,8这8个数字，则最小的4个数字是1,2,3,4,。

vector<int> GetLeastNumbers_Solution(vector<int> input, int k) {
    vector<int> v;
    if(k>input.size())
        return v;
    int index_min;
    for(int i=0; i<k; i++){
        index_min = i;
        for(int j=i+1; j<input.size(); j++){
            if(input[j]<input[index_min]){
                index_min = j;
            }
        }
        swap(input[i], input[index_min]);
    }
    for(int i=0; i<k; i++){
        v.push_back(input[i]);
    }
    return v;
}
