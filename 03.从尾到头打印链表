从尾到头打印链表
输入一个链表，按链表值从尾到头的顺序返回一个ArrayList。

//不动链表
vector<int> printListFromTailToHead(ListNode* head) {
    vector<int> res;
    ListNode* pNode = head;
    while (pNode) {
        res.push_back(pNode->val);
        pNode = pNode->next;
    }
    for (int i = 0, j = res.size() - 1; i < j; i++, j--) {
        swap(res[i], res[j]);
    }
    return res;
}

//原地逆转链表
vector<int> printListFromTailToHead(ListNode* head) {
    vector<int> a;
    if(head)
        return a;
    ListNode* pre=head;
    ListNode* buf=head;
    while(head->next){
        buf = head->next;
        head->next = buf->next;
        buf->next = pre;
        pre = buf;
    }
    while(buf){
        a.push_back(buf->val);
        buf = buf->next;
    }
    return a;
}
